{
  "expo": {
    "name": "Meals App",
    "slug": "meals-app",
    "version": "1.0.0",
    "description": "This is a practice meals app",
    "orientation": "portrait",
    "icon": "./assets/react-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/react-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
